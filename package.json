{
  "name": "test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "serve": "npx webpack serve",
    "build": "npx webpack",
    "lint-js": "eslint --ignore-path .eslintignore --ignore-pattern \"!**/.*\" .",
    "lint-css": "stylelint \"src/**/*.{css,less,scss,sss}\"",
    "lint": "yarn run lint-js && yarn run lint-css",
    "fix-js": "yarn run lint-js -- --fix",
    "fix-css": "yarn run lint-css -- --fix",
    "fix": "yarn run fix-js && yarn run fix-css",
    "test": "jest --config='jest.config.ts' --runInBand",
    "test-watch": "yarn run test -- --watch --notify",
    "test-cover": "yarn run test -- --coverage",
    "coverage": "yarn run test-cover && opn coverage/lcov-report/index.html",
    "clean": "babel-node tools/run clean"
  },
  "engines": {
    "node": ">=6.5",
    "npm": ">=3.10"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@better-scroll/core": "^2.4.2",
    "@better-scroll/observe-dom": "^2.4.2",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^2.6.2",
    "rxjs": "^7.5.4",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4"
  },
  "dependencies": {
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-whooks": "^0.1.1",
    "umi-request": "^1.4.0",
    "zyj-utils": "0.0.4"
  }
}
